{
  "name": "pingus",
  "version": "1.1.14",
  "description": "A simple network ping tool. Supports TCP / UDP / ICMP protocol.",
  "author": "Wany <sung@wany.io> (https://wany.io)",
  "main": "lib/index.js",
  "license": "MIT",
  "exports": {
    "require": "./lib/index.js",
    "import": "./src/index.mjs"
  },
  "keywords": [
    "ping",
    "icmp",
    "tcp",
    "udp",
    "port",
    "scan",
    "scanner",
    "banner",
    "bannergrab",
    "traceroute",
    "wol",
    "net",
    "dgram"
  ],
  "homepage": "https://github.com/wnynya/Pingus#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/wnynya/Pingus.git"
  },
  "scripts": {
    "test": "sudo node test/test.mjs",
    "pingus": "sudo node src/index.mjs --pingus-cli",
    "babel": "npx babel src --out-file-extension .js --out-dir lib"
  },
  "dependencies": {
    "@wnynya/dns": "^0.7.0",
    "@wnynya/ip": "^0.8.0",
    "raw-socket": "^1.7.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.8",
    "@babel/core": "^7.14.8",
    "@babel/plugin-transform-modules-commonjs": "^7.18.2",
    "@babel/plugin-transform-runtime": "^7.18.2",
    "@babel/preset-env": "^7.14.9",
    "babel-plugin-replace-import-extension": "^1.1.3",
    "babel-plugin-transform-import-meta": "^2.2.0"
  }
}
